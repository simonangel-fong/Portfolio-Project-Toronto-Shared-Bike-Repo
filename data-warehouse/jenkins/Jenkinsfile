pipeline {
  agent any
  stages
  {
    stage('Start PostgreSQL') {
      steps {
        sh '''
        docker --version
        docker compose -f data-warehouse/postgresql/docker-compose.yaml down
        docker compose -f data-warehouse/postgresql/docker-compose.yaml up -d --build
        '''
      }
    }
    stage('Extract Data') {
      steps {
        sh '''
        docker exec -it postgresql hostname
        '''
      }
    }
